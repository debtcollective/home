    "gatsby-transformer-sharp": "^2.5.13",
    "html-react-parser": "^1.2.6",
    "js-cookie": "^2.2.1",
    "lodash.chunk": "^4.2.0",
    "material-icons": "^0.7.0",
    "node-fetch": "^2.6.1",
    "postcss": "^8.3.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "pure-react-carousel": "^1.27.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.11.2",
    "react-phone-input-2": "^2.13.8",
    "react-share": "^4.4.0",
    "react-youtube": "^7.12.0",
